#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	int edad;
	cout<<"ingrese edad: ";
	cin>> edad;
	if (edad >=18)
		cout<<"Puede votar";
	else
		cout<<"NO puede votar";
	getch();
}
