#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
	float b,h,area;
	cout <<"ingrese el valor de la base";
	cin>>b;
	cout <<"ingrese valor de altura";
	cin>>h;
	area= 1/2(b*h);

	cout <<"el area es: "<<area;
	getch();
}
